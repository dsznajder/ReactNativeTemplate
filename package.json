{
  "name": "TemplateProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn react-native run-android",
    "bootstrap": "./scripts/bootstrap.sh",
    "clean:all": "yarn clean:watchman && yarn clean:builds && yarn clean:gradlew && yarn clean:cache && yarn clean:packages",
    "clean:builds": "rm -rf ios/build android/build android/app/build",
    "clean:cache": "rm -rf $TMPDIR/react-* $TMPDIR/haste-map-react-native-packager-* ~/.rncache",
    "clean:gradlew": "cd android && ./gradlew clean && cd ..",
    "clean:packages": "rm -rf ios/Pods && rm -rf node_modules && yarn bootstrap",
    "clean:watchman": "watchman watch-del-all",
    "coverage": "open coverage/lcov-report/index.html",
    "ios": "yarn react-native run-ios --simulator='iPhone 11'",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "postinstall": "./scripts/postinstall.sh",
    "start": "react-native start",
    "test": "jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test:unit": "jest -u --runInBand",
    "test:watch": "jest --watch",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.9.0",
    "@react-native-community/masked-view": "0.1.7",
    "@react-native-community/netinfo": "5.7.0",
    "@react-native-community/status-bar": "1.0.3",
    "@react-navigation/bottom-tabs": "5.2.5",
    "@react-navigation/native": "5.1.4",
    "@react-navigation/native-stack": "5.0.5",
    "@types/i18n-js": "3.0.1",
    "i18n-js": "3.5.1",
    "react": "16.13.1",
    "react-native": "0.62.0",
    "react-native-config": "1.0.0",
    "react-native-gesture-handler": "1.6.1",
    "react-native-keyboard-manager": "4.0.13-16",
    "react-native-reanimated": "1.7.1",
    "react-native-redash": "10.1.1",
    "react-native-screens": "2.4.0",
    "react-native-vector-icons": "6.6.0",
    "use-memo-one": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/runtime": "7.9.2",
    "@types/jest": "25.1.4",
    "@types/lodash": "4.14.149",
    "@types/react": "16.9.31",
    "@types/react-native": "0.62.1",
    "@types/react-test-renderer": "16.9.2",
    "babel-jest": "25.2.4",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "6.8.0",
    "eslint-config-satya164": "3.1.6",
    "eslint-plugin-sort-imports-es6-autofix": "0.5.0",
    "flipper-plugin-react-native-performance": "0.4.3",
    "husky": "4.2.3",
    "jest": "25.2.4",
    "lint-staged": "10.1.1",
    "metro-react-native-babel-preset": "0.59.0",
    "prettier": "2.0.2",
    "react-test-renderer": "16.13.1",
    "typescript": "3.8.3"
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "typescript",
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "jest": {
    "preset": "react-native",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/assets/"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js",
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js",
      "<rootDir>/jest/__mocks__/react-native-reanimated.mock.js",
      "<rootDir>/jest/__mocks__/react-native-gesture-handler.mock.js",
      "<rootDir>/jest/__mocks__/react-native-config.mock.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "testPathIgnorePatterns": [
      "__e2e__",
      "node_modules"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn typescript"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext ',.ts,.tsx,.js'"
    ]
  }
}
