{
  "name": "TemplateProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn react-native run-android",
    "bootstrap": "./scripts/bootstrap.sh",
    "clean:all": "yarn clean:watchman && yarn clean:builds && yarn clean:gradlew && yarn clean:cache && yarn clean:packages",
    "clean:builds": "rm -rf ios/build android/build android/app/build",
    "clean:cache": "rm -rf $TMPDIR/react-* $TMPDIR/haste-map-react-native-packager-* ~/.rncache",
    "clean:gradlew": "cd android && ./gradlew clean && cd ..",
    "clean:packages": "rm -rf ios/Pods && rm -rf node_modules && yarn bootstrap",
    "clean:watchman": "watchman watch-del-all",
    "coverage": "open coverage/lcov-report/index.html",
    "ios": "yarn react-native run-ios --simulator='iPhone 11'",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "postinstall": "./scripts/postinstall.sh",
    "start": "react-native start",
    "test": "jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test:unit": "jest -u --runInBand",
    "test:watch": "jest --watch",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.7.1",
    "@react-native-community/masked-view": "0.1.6",
    "@react-native-community/netinfo": "5.3.3",
    "@react-native-community/status-bar": "1.0.3",
    "@react-navigation/bottom-tabs": "5.0.0",
    "@react-navigation/native": "5.0.0",
    "@react-navigation/native-stack": "5.0.0",
    "@types/i18n-js": "3.0.1",
    "i18n-js": "3.5.1",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-device-info": "5.5.1",
    "react-native-gesture-handler": "1.5.6",
    "react-native-localize": "1.3.3",
    "react-native-reanimated": "1.7.0",
    "react-native-redash": "9.6.0",
    "react-native-screens": "2.0.0-beta.2",
    "react-native-vector-icons": "6.6.0",
    "use-memo-one": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/runtime": "7.8.4",
    "@types/jest": "25.1.2",
    "@types/lodash": "4.14.149",
    "@types/react": "16.9.19",
    "@types/react-native": "0.61.12",
    "@types/react-test-renderer": "16.9.2",
    "babel-jest": "25.1.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "6.8.0",
    "eslint-config-satya164": "3.1.5",
    "eslint-plugin-sort-imports-es6-autofix": "0.5.0",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "metro-react-native-babel-preset": "0.58.0",
    "prettier": "1.19.1",
    "react-test-renderer": "16.12.0",
    "typescript": "3.7.5"
  },
  "jest": {
    "preset": "react-native",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/assets/"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js",
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js",
      "<rootDir>/jest/__mocks__/react-native-reanimated.mock.js",
      "<rootDir>/jest/__mocks__/react-native-gesture-handler.mock.js",
      "<rootDir>/jest/__mocks__/react-native-device-info.mock.js",
      "<rootDir>/jest/__mocks__/react-native-localize.mock.js",
      "<rootDir>/jest/__mocks__/react-native-netinfo.mock.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native)"
    ],
    "testPathIgnorePatterns": [
      "__e2e__",
      "node_modules"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn typescript"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext ',.ts,.tsx'"
    ]
  }
}
