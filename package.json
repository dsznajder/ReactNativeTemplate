{
  "name": "TemplateProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "bootstrap": "./scripts/bootstrap.sh",
    "clean": "rm -rf ~/.rncache && watchman watch-del-all && yarn remove-builds",
    "generate-apk": "cd android && ./gradlew assembleRelease",
    "lint": "eslint ./ --ext .js",
    "remove-builds": "rm -rf ios/build android/build android/app/build",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest -u",
    "test:ci": "jest --coverage --runInBand --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "lodash": "4.17.10",
    "prop-types": "15.6.2",
    "react": "16.5.0",
    "react-native": "0.57.0",
    "react-native-vector-icons": "5.0.0",
    "react-navigation": "2.14.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-plugin-lodash": "3.3.4",
    "metro-react-native-babel-preset": "0.45.2",
    "eslint": "5.5.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.22.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-react-native": "3.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.3.0",
    "husky": "1.0.0-rc.13",
    "jest": "23.6.0",
    "lint-staged": "7.2.2",
    "prettier": "1.14.2",
    "react-test-renderer": "16.5.0"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ],
    "testPathIgnorePatterns": [
      "__e2e__",
      "node_modules"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  }
}
